<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
						http://www.springframework.org/schema/beans/spring-beans.xsd">

	<util:list id="stockList" value-type="com.jpmorgan.stocks.model.stock.Stock">
		<ref bean="stockTEA"/>
		<ref bean="stockPOP"/>
		<ref bean="stockALE"/>
		<ref bean="stockGIN"/>
		<ref bean="stockJOE"/>
	</util:list>
	
	<bean name="stockTEA" class="com.jpmorgan.stocks.model.stock.CommonStock">
		<constructor-arg index="0" value="TEA" type="java.lang.String"/>
		<constructor-arg index="1" value="0" type="java.math.BigDecimal"/>
		<constructor-arg index="2" value="100" type="java.math.BigDecimal"/>
	</bean>
	
	<bean name="stockPOP" class="com.jpmorgan.stocks.model.stock.CommonStock">
		<constructor-arg index="0" value="POP" type="java.lang.String"/>
		<constructor-arg index="1" value="8" type="java.math.BigDecimal"/>
		<constructor-arg index="2" value="100" type="java.math.BigDecimal"/>
	</bean>
	
	<bean name="stockALE" class="com.jpmorgan.stocks.model.stock.CommonStock">
		<constructor-arg index="0" value="ALE" type="java.lang.String"/>
		<constructor-arg index="1" value="23" type="java.math.BigDecimal"/>
		<constructor-arg index="2" value="60" type="java.math.BigDecimal"/>
	</bean>
	
	<bean name="stockGIN" class="com.jpmorgan.stocks.model.stock.PreferredStock">
		<constructor-arg index="0" value="GIN" type="java.lang.String"/>
		<constructor-arg index="1" value="8" type="java.math.BigDecimal"/>
		<constructor-arg index="2" value="2" type="java.math.BigDecimal"/>
		<constructor-arg index="3" value="100" type="java.math.BigDecimal"/>
	</bean>
	
	<bean name="stockJOE" class="com.jpmorgan.stocks.model.stock.CommonStock">
		<constructor-arg index="0" value="JOE" type="java.lang.String"/>
		<constructor-arg index="1" value="13" type="java.math.BigDecimal"/>
		<constructor-arg index="2" value="250" type="java.math.BigDecimal"/>
	</bean>

</beans>
